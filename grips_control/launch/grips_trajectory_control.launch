<launch>
  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find grips_control)/config/joint_trajectory_control.yaml" command="load"/>
  <!-- load the controllers -->
  <node name="grips_controller" pkg="controller_manager" type="spawner" output="screen" 
      args="trajectory_controller joint_state_controller"/>
</launch>
