<launch>
	<arg name="robot_name" value="grips_simple_gripper" />
	<!-- the semantic description that corresponds to the URDF -->
	<param name="robot_description_semantic" textfile="$(find grips_moveit_config)/config/$(arg robot_name).srdf" />
	 
	<node name="$(arg robot_name)_motion_control" pkg="grips_control" type="motion_control" respawn="false" output="screen" ns="$(arg robot_name)">
		<rosparam command="load" file="$(find grips_moveit_config)/config/kinematics.yaml"/>
	</node>
	
</launch>
